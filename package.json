{
  "name": "golftracker",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "clean": "watchman watch-del-all && rm -rf $TMPDIR/react-*",
    "lint": "eslint src",
    "test": "jest",
    "storybook": "storybook start -p 7007",
    "ios": "react-native run-ios --simulator=\"iPhone 8\"",
    "ios:storybook": "react-native run-ios --simulator=\"iPhone 8\" --scheme Storybook",
    "web": "webpack --env.development --env.vendor && webpack-dev-server --env.development",
    "web:test": "webpack --env.test --env.vendor && webpack --env.test --progress",
    "web:prod": "webpack --env.production --env.vendor && webpack --env.production --progress",
    "web:serve": "http-serve -p 3001 --gzip true ./build"
  },
  "dependencies": {
    "dot-prop-immutable": "^1.3.1",
    "geolib": "^2.0.23",
    "google-map-react": "^0.24.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-native": "0.49.3",
    "react-native-gps": "https://github.com/agrass/react-native-gps",
    "react-native-maps": "^0.17.0",
    "react-native-web": "^0.1.9",
    "react-navigation": "^1.0.0-beta.13",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-persist": "^4.10.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.2",
    "babel-eslint": "^8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.1.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-react-native": "^3.1.0",
    "glob": "^7.1.2",
    "happypack": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "http-serve": "^1.0.1",
    "interpolate-html-plugin": "^2.0.0",
    "jest": "21.2.1",
    "json-loader": "^0.5.7",
    "lodash-webpack-plugin": "^0.11.4",
    "offline-plugin": "^4.8.4",
    "react-hot-loader": "^3.1.0",
    "react-test-renderer": "^16.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
